A.selectionStart=B.length;
A.selectionEnd=C.length;
this.input.adjust()
}else{A.value=B
}},current:function(){if(this.selection){var A=this.input.id(this.selection);
return A
}},test:function(A,B){return A.test(B)
},rebuild:function(C){var B=this.node.input;
