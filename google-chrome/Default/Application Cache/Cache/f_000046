}}catch(F){}B.value=G;
D()
}else{try{if(B.password){B.type="password"
}}catch(F){}B.removeClass("hint")
}B.fire("input")
}function A(F){if(F==undefined){return this.hasClass("hint")?"":this.value
}this.value=F;
C(null,this)
}B.on("focus",E);
B.on("blur",C);
B.valueOf=A;
B.valueEx=A;
B.password=B.type=="password";
C(null,B)
};
