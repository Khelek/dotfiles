var actual_box = '';

if (location.hash != '' && location.hash != '#base_domain=deezer.com' && location.hash != '#_=_' && location.hash.substr(0,6) != '#inapp') { // Fix FB hash link
	var loc = location.hash.substr(1, location.hash.length - 1);
	if (loc.charAt(0) == "/") {
		loc = loc.substr(1, location.hash.length - 1);
	}

	var tab = loc.split('?');

	var allowed_locs = ['discography', 'related', 'review', 'video', 'biography', 'chronicles', 'tracks', 'fan'];
	if ($.inArray(loc, allowed_locs) === -1) {

		if (tab.length == 2) {
			document.location.href = 'http://' + SETTING_HOST + '/' + SETTING_LANG + '/' + tab[0] + '?' + hex2bin(tab[1]);
		} else {
			document.location.href = 'http://' + SETTING_HOST + '/' + SETTING_LANG + '/' + loc;
		}
	}
}

