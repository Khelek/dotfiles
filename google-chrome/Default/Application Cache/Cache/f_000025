<script type="text/javascript">
$(document).ready(function() {
	if (facebook.commentEnable()) {
		$('#fb_perms_comment').addClass('active');
	} else {
		$('#fb_perms_comment').removeClass('active');
	}

	if (twitter.commentEnable()) {
		$('#tw_perms_comment').addClass('active');
	} else {
		$('#tw_perms_comment').removeClass('active');
	}

	if (USER.USER_ID == 0) {

		$("#tab_review_content .like").attr('onClick', "modal.open('/" + SETTING_LANG + "/login.php')");
		$("#tab_review_content .dislike").attr('onClick', "modal.open('/" + SETTING_LANG + "/login.php')");

		$('#naboo_comment_new').hide();
		$('#naboo_comment_new_not_logged').show();

	} else{

		$('#naboo_comment_new').show();
		$('#naboo_comment_new_not_logged').hide();

		$('#tab_review_content .like').tipsy({gravity: 's', space : 10});
		$('#tab_review_content .dislike').tipsy({gravity: 's', space : 10});
	}

	});
</script>			</div>
